// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// CONFLICTS

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique

  // CONFLICTS keep
  name     String?
  password String
}

